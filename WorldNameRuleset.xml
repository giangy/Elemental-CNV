<?xml version="1.0" encoding="ISO-8859-1" ?>
<NameGeneratorRuleset InternalName="WorldNameRuleset">
	<DataChecksum NoParse="1">
		<Ignore>DisplayName</Ignore>
		<Translate>DisplayName,TerrainName</Translate>
	</DataChecksum>
	<MinimumLetterLength>5</MinimumLetterLength>
	<MaximumLetterLength>10</MaximumLetterLength>
    <DefaultGenerationString>{S}</DefaultGenerationString>

	<!-- Name Generation Rules (Change as desired). -->
	<ProductionRule>
		<Variable>S</Variable>
		<Replace>{Vowel}{A}</Replace>
		<Probability>0.5</Probability>
	</ProductionRule>
	<ProductionRule>
		<Variable>S</Variable>
		<Replace>{Consonant}{B}</Replace>
		<Probability>0.5</Probability>
	</ProductionRule>
	<ProductionRule>
		<Variable>A</Variable>
		<Replace>{Consonant}{B}</Replace>
		<Probability>0.75</Probability>
	</ProductionRule>
  <ProductionRule>
    <Variable>A</Variable>
    <Replace>{DoubleConsonant}{B}</Replace>
    <Probability>0.25</Probability>
  </ProductionRule>
	<ProductionRule>
		<Variable>B</Variable>
		<Replace>{Vowel}{A}</Replace>
		<Probability>0.75</Probability>
	</ProductionRule>
  <ProductionRule>
    <Variable>B</Variable>
    <Replace>{DoubleVowel}{A}</Replace>
    <Probability>0.25</Probability>
  </ProductionRule>
  

